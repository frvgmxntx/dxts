{
  "battery": {
    "format": "{icon}",
    "format-icons": ["","","","",""],
    "states": {
      "mid": 50,
      "low": 10,
    },
    "tooltip": false,
  },

  "battery#charge": {
    "format-discharging": "{capacity}%",
    "format-charging": "{capacity}% ",
    "format-pending-charging": "{capacity}% ",
    "full-at": 79,
    "tooltip": false,
  },

  "group/battery": {
    "orientation": "inherit",
    "drawer": {
      "transition-duration": 600,
      "children-class": "battery-child",
      "transition-left-to-right": false,
    },
    "modules": [
      "battery",
      "battery#charge",
    ],
  },

}
